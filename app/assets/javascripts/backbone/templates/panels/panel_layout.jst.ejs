<% if(title){ %>
<header>
	<h3><% if(icon){ %><i class="panel-icon bf-icon <%= icon %>"></i><% } %><%= title %></h3>
</header>
<% } %>
<div class="panel-content">
</div>
<% if(panelTips){ %>
<footer>
	<p class="tips"><%= panelTips.text %><% if(panelTips.link){ %> - <a target="_blank" href="<%= panelTips.link.url %>"><%= panelTips.link.short %></a><% } %></p>
</footer>
<% } %>